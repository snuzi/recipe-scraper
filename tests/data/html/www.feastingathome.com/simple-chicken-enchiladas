 .cp-target { }.cp_style_36042 #cp_button-4-36042 .cp-target ~ .cp-field-shadow { }.cp_style_36042 #cp_button-4-36042 .cp-rotate-wrap{ transform:rotate( 0deg);}.cp_style_36042 #cp_button-4-36042 .cp-target:hover { }.cp_style_36042 #cp_button-4-36042 .cp-target:hover ~ .cp-field-shadow { }.cp_style_36042 #cp_button-4-36042 { left: 224.5px;top: 223px;z-index:7;}@media ( max-width: 799px ) {.cp_style_36042 #cp_button-4-36042 .cp-target { font-family:inherit;font-style:Inherit;font-weight:Inherit;font-size:14px;letter-spacing:0px;text-align:center;color:#fff;background:#333333;width:328px;height:37px;padding:0px 15px 0px 15px;}.cp_style_36042 #cp_button-4-36042 .cp-target:hover { color:#fff;background:#333333;}.cp_style_36042 #cp_button-4-36042 .cp-target { border-style:none;}.cp_style_36042 #cp_button-4-36042 .cp-target ~ .cp-field-shadow { border-style:none;}.cp_style_36042 #cp_button-4-36042 .cp-target { border-color:#757575;}.cp_style_36042 #cp_button-4-36042 .cp-target ~ .cp-field-shadow { border-color:#757575;}.cp_style_36042 #cp_button-4-36042 .cp-target { border-width:1px 1px 1px 1px;}.cp_style_36042 #cp_button-4-36042 .cp-target ~ .cp-field-shadow { border-width:1px 1px 1px 1px;}.cp_style_36042 #cp_button-4-36042 .cp-target { border-radius:0px 0px 0px 0px;}.cp_style_36042 #cp_button-4-36042 .cp-target ~ .cp-field-shadow { border-radius:0px 0px 0px 0px;}.cp_style_36042 #cp_button-4-36042 .cp-target > .cp-close-link { border-radius:0px 0px 0px 0px;}.cp_style_36042 #cp_button-4-36042 .cp-target > .cp-close-image { border-radius:0px 0px 0px 0px;}.cp_style_36042 #cp_button-4-36042 .cp-target { }.cp_style_36042 #cp_button-4-36042 .cp-target ~ .cp-field-shadow { }.cp_style_36042 #cp_button-4-36042 .cp-rotate-wrap{ transform:rotate( 0deg);}.cp_style_36042 #cp_button-4-36042 .cp-target:hover { }.cp_style_36042 #cp_button-4-36042 .cp-target:hover ~ .cp-field-shadow { }.cp_style_36042 #cp_button-4-36042 { left: 14px;top: 466px;z-index:7;}}@media ( max-width: 799px ) {.cp_style_36042 .cp-invisible-on-mobile {display: none !important;}}</style>
					<div class="cp-popup-content cpro-active-step  cp-before_after    cp-middle  cp-panel-1"   data-overlay-click ="1" data-title="After Posts" data-module-type="before_after"  data-step="1"  data-width="681" data-mobile-width="360" data-height="286" data-mobile-height="530" data-mobile-break-pt="799" data-popup-position="middle" data-mobile-responsive="yes">
												
					<div class="cpro-form-container">                             
						<div id="cp_image-4-36042" class="cp-field-html-data    cp-none cp-image-ratio" data-type="cp_image"  data-action="none" data-step="1"     >
	<div class="cp-rotate-wrap">
		<div class="cp-image-main"><img data-cp-src="https://www.feastingathome.com/wp-content/uploads/2020/08/Facetune.jpg" class="cp-img-lazy cp-target cp-field-element cp-image" name="cp_image-4" alt="Sylvia Fountaine, Feasting at Home Blog" src="">
			<div class="cp-field-shadow"></div>
		</div>
		
	</div>
